{
    "name": "siemens840d",
    "displayName": "siemens840d",
    "description": "Siemens 840D Language Support",
    "version": "0.0.2",
    "publisher": "SScipione",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": ["Programming Languages"],
    "contributes": {
        "languages": [{
            "id": "s840d",
            "aliases": ["S840D", "s840d", "GCODE"],
            "extensions": [".spf",".mpf",".def",".tea", ".iso", ".nc", ".cnc", ".km2", ".arc", ".tpl", ".cmc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "s840d",
            "scopeName": "source.s840d",
            "path": "./syntaxes/s840d.tmLanguage.json"
        }]
    }
}
