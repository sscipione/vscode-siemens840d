            "patterns": [            {
                "name": "constant.numeric.s840d.nr",
                "match": "\\b[0-9]+\\b"
            },
            {
                "name": "constant.numeric.s840d.hide",
                "match": "\\n\\s*\\/[0-7]?"
            },
            {
                "name": "constant.numeric.s840d.linenumber",
                "match": "\\bN[0-9]+\\b"
            },
            {
                "name": "constant.numeric.s840d.label",
                "match": "__.+:"
            }]
        },
        "operators": {
            "name": "keyword.operator.s840d",
            "patterns": [
                { "include": "#assignment-operator" },
                { "include": "#arithmetik-operators" },
                { "include": "#comparison-operators" },
                { "include": "#string-operators" }
            ]
        },
        "assignment-operator": {
            "patterns": [{
                "name": "keyword.operator.s840d",
                "match": "\\b=\\b"
