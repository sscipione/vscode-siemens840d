            {
                "name": "keyword.operator.bool.s840d",
                "match": "not|b_not|b_and|b_xor|b_or|and|xor|or"
            }]
        },
        "comparison-operator": {
            "patterns": [{
                "name": "keyword.operator.comparison.s840d",
                "match": "==|<|>|>=|<=|<>"
            }]
        },
        "string-operator": {
            "patterns": [{
                "name": "keyword.operator.string.s840d",
                "match": "<<"
            }]
        }
